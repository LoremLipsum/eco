mixin callback(data)
    if data.iconColor == 'primary'
        +el({
            class: 'callback',
            mix: data.mix,
            mod: data.mod,
            js: data.js,
        })&attributes(attributes)
            +button-icon({
                tag: 'a',
                mod: 'primary',
                mix: 'callback__icon',
                href: 'tel:88005053848',
                ariaLabel: 'Позвонить',
                icon: 'tel',
            })
            +el({
                tag: 'a',
                href: 'tel:88005053848',
                class: 'callback__link',
                text: '8 800 <span class="callback__number">505 38 48</span>',
            })
            +el({
                class: 'callback__text',
                text: 'Бесплатно по всей Росcии',
            })
    else
        +el({
            class: 'callback',
            mix: data.mix,
            mod: data.mod,
            js: data.js,
        })&attributes(attributes)
            +button-icon({
                tag: 'a',
                mod: 'white',
                mix: 'callback__icon',
                href: 'tel:88005053848',
                ariaLabel: 'Позвонить',
                icon: 'tel',
            })
            +el({
                tag: 'a',
                href: 'tel:88005053848',
                class: 'callback__link',
                text: '8 (800) <span class="callback__number">505 38 48</span>',
            })
            +el({
                class: 'callback__text',
                text: 'Бесплатно по всей Росcии',
            })
