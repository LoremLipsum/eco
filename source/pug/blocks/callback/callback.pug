mixin callback(data)
    +el({
        class: 'callback',
        mix: data.mix,
        mod: data.mod,
        js: data.js,
    })&attributes(attributes)
        +button-icon({
            tag: 'button',
            mix: 'callback__button',
            ariaLabel: 'Окрыть форму обратного звонка',
            icon: 'tel',
        })
        +el({
            tag: 'a',
            href: 'tel:88005053848',
            class: 'callback__link',
            text: '8 800 <span class="callback__number">505 38 48</span>',
        })
        +el({
            class: 'callback__text',
            text: 'Бесплатно по всей Росcии',
        })
