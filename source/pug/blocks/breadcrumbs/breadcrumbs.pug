mixin breadcrumbs(data)
    +el({
        tag: 'nav',
        class: 'breadcrumbs',
        mod: data.mod,
        mix: data.mix,
        js: data.js,
    })
        +el({
            tag: 'ul',
            class: 'breadcrumbs__list',
        })
            each item in data.items
                +el({
                    tag: 'li',
                    class: 'breadcrumbs__item',
                })
                    +el({
                        tag: 'a',
                        class: 'breadcrumbs__link',
                        text: item,
                    })
